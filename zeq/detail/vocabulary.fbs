namespace zeq.vocabulary;

//A struct containing restNames, eventHighs, eventLows, schemas would be more
//clear but flatbuffers does not support strings in structures.
table Vocabulary
{
  restNames:[string];
  eventHighs:[ulong];
  eventLows:[ulong];
  schemas:[string];
}

root_type Vocabulary;
